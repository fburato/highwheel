    Parser -> Util
    Modules -> Core
    Modules -> Model
    Modules -> Util
    Modules -> Parser
    ModulesCli -> Modules
    ModulesCli -> Util
    HighwheelMavenMain -> Model
    HighwheelMavenMain -> Util
    HighwheelMavenMain -> Core
    HighwheelMavenMain -> Parser
    ModulesMaven -> Modules
    ModulesMaven -> Util
    Ant -> Model
    Ant -> Util
    Ant -> Core
    Ant -> Parser
    Bytecode -> Model
    Bytecode -> Util
    Bytecode -> Core
    Bytecode -> Parser
